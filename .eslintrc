{
  "extends": "airbnb/legacy",
  "rules": {
    "brace-style": [2, "stroustrup"],
    "comma-dangle": 0,
    "consistent-return": 0,
    "curly": 0,
    "func-names": 0,
    "indent": 0,
    "no-console": 0,
    "no-param-reassign": 0,
    "object-curly-spacing": 0,
    "one-var": 0,
    "padded-blocks": 0,
    "space-before-function-paren": 0,
    "vars-on-top": 0
  }
}
